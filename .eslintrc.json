{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["error", { "varsIgnorePattern": "AlgoTheses" }]
  },
  "globals": {
    "application": "readonly"
  }
}
